<div>
    <h1>¡Hola, {{user.name}} {{user.last_name}}! ¡Que gusto verte!</h1>
    <p>Tu edad es {{user.age}}</p>
    <p>Tu correo {{user.email}}</p>
    <p>Tu telefono {{user.tel}}</p>

    {{#if isAdmin}}
        {{#each food}}
            <div>
                <p>{{this.name}}</p>
                <p>{{this.price}}</p>
            </div>
        {{/each}}
    {{else}}
        <p>No tienes permisos para ver la lista de alimentos</p>
    {{/if}}
</div>
<script src="/js/index.js"></script>